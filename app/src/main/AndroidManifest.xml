<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".main.SplitWise"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SplitWise"
        tools:targetApi="31">
        <activity
            android:name=".Core.Profile.ui.home.AddExpenseActivity"
            android:exported="false" />
        <activity
            android:name=".Core.Profile.ProfileNavVC"
            android:exported="false"
            android:label="@string/title_activity_profile_nav_vc"
            android:theme="@style/Theme.SplitWise.NoActionBar" />
        <activity
            android:name=".Core.CoreVC"
            android:exported="false" />
        <activity
            android:name=".Core.Register.RegisterVC"
            android:exported="false" />
        <activity
            android:name=".Core.Login.LoginVC"
            android:exported="false" />
        <activity
            android:name=".Core.Launch.MainFragment"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>